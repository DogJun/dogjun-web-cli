{
  "name": "dogjun-cli",
  "version": "1.0.0",
  "description": "DogJun脚手架",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "npm run server:prod && npm run client:prod",
    "server:dev": "cross-env NODE_ENV=development gulp",
    "server:prod": "cross-env NODE_ENV=production gulp",
    "client:dev": "webpack --mode development --module-bind js=babel-loader",
    "client:prod": "webpack --mode production --module-bind js=eslint-loader!babel-loader",
    "start:dev": "cross-env NODE_ENV=development supervisor ./dist/app.js",
    "docs": "jsdoc ./src/nodeuii/services/UserService.js -d docs/jsdocs",
    "service": "mocha ./tests/mochaRunner.js",
    "permance": "webpack --mode production --profile --json --config ./webpack.config.js > ./test/permance/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DogJun/dogjun-cli.git"
  },
  "keywords": [],
  "author": "DogJun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DogJun/dogjun-cli/issues"
  },
  "homepage": "https://github.com/DogJun/dogjun-cli#readme",
  "dependencies": {
    "co": "^4.6.0",
    "cross-env": "^5.1.4",
    "koa": "^2.5.0",
    "koa-simple-router": "^0.2.0",
    "koa-static": "^4.0.2",
    "koa-swig": "^2.2.1",
    "lodash": "^4.17.5",
    "log4js": "^2.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.42",
    "awilix": "^3.0.6",
    "awilix-koa": "^2.1.1",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "eslint": "^5.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0-beta.2",
    "gulp-eslint": "^5.0.0",
    "gulp-rollup": "^2.16.2",
    "gulp-sequence": "^1.0.0",
    "gulp-watch": "^5.0.0",
    "html-minifier": "^3.5.13",
    "html-webpack-plugin": "^3.0.7",
    "jsdoc": "^3.5.5",
    "koa-convert": "^1.2.0",
    "koa-liveload": "^0.0.2",
    "koa-webpack-dev-middleware": "^2.0.2",
    "koa-webpack-hot-middleware": "^1.0.3",
    "koa-webpack-middleware": "^1.0.7",
    "koa2-connect": "^1.0.2",
    "mocha": "^5.0.4",
    "mochawesome": "^3.0.2",
    "postcss-cssnext": "^3.1.0",
